#Ass-1To print given number is prime or not
n=int(input('Enter a number:'))
i=1
c=0
while(i<=n):
    if(n%i==0):
        c=c+1
    i=i+1
if(c==2):
    print('Prime Number')
else:
    print('Not a Prime Number')

#Ass-2
a = int(input())
b = int(input())
print(a+b)
print(a-b)
print(a*b)

#Ass-3
n = int(input())
for i in range(1,n+1):
    print(i,end="")
